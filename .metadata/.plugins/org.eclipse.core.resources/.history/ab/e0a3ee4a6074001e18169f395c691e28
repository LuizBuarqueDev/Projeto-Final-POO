package ifpe.sistemas;

import java.util.List;

public class BancoDeDados {
	
	private List<String> banco;
		
	public BancoDeDados(List<String> banco) {
		super();
		this.banco = banco;
	}

	public String ler() {
		return "BancoDeDados [banco=" + banco + "]";
	}
	
	public void gravar(String entrada) {
		 banco.add(entrada);
	}
}
